{
  "manifest_version": 3,
  "name": "FullPageTitle",
  "version": "1.0.1",
  "description": "Display the full title of the current page.",
  "permissions": ["scripting", "activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_title": "FullPageTitle"
  },
  "host_permissions": ["<all_urls>"],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
